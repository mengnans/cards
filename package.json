{
  "name": "cards",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-lines-ellipsis": "^0.13.2",
    "react-redux": "^5.1.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^5.1.1"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "lint-fix": "eslint --fix ./src/js/*/*.js",
    "lint": "eslint ./src/js/*/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/test/setup/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/mock/styleMock.js"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "babel-polyfill": "^7.0.0-beta.3",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
